<h1>LOGIN</h1>

<%= form_tag login_index_path do %>
   <ul>
    <li>
      <%= label_tag :username %>
      <%= text_field_tag :username %><br>
    </li>
    <br>
    <li>
      <%= label_tag :password %>
      <%= password_field_tag :password %><br>
    </li>
  </ul>
  <%= submit_tag "LOGIN" %>
<% end %>
